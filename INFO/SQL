CREATE TABLE IF NOT EXISTS users
( id INTEGER NOT NULL AUTO_INCREMENT,
  first_name VARCHAR(128) NOT NULL,
  last_name VARCHAR(128) NOT NULL,
  phone VARCHAR(56) NOT NULL,
  PRIMARY KEY (id)
);
USE bi3sdfkm9nrjwecgdorh;
CREATE TABLE IF NOT EXISTS users_data_table (
  chat_id BIGINT PRIMARY KEY,
      first_name VARCHAR(255),
      last_name VARCHAR(255),
      user_name VARCHAR(255),
      phone VARCHAR(20),
      registered_at TIMESTAMP
      PRIMARY KEY (id)
);
CREATE TABLE users (
    chat_id BIGINT PRIMARY KEY,
    first_name VARCHAR(255),
    last_name VARCHAR(255),
    user_name VARCHAR(255),
    phone VARCHAR(20),
    registered_at TIMESTAMP
    PRIMARY KEY (id)
);


GRANT ALL PRIVILEGES
ON bot_db.*
TO 'dev'@'localhost'
WITH GRANT OPTION;


use bot_db;
show tables;
select * from users_data_table;



CREATE TABLE `bot_db`.`ads_table` (
    `id` BIGINT AUTO_INCREMENT PRIMARY KEY,
    `ad` VARCHAR(255)
);





INSERT INTO `ads_table`  VALUES (3,  '–¢–µ—Å—Ç3');





CREATE TABLE `bot_db`.`parfums_table` (
  id INTEGER NOT NULL AUTO_INCREMENT,
  img VARCHAR(255) NOT NULL,
  name VARCHAR(255) NOT NULL,
  gender VARCHAR(255) NOT NULL,
  article VARCHAR(255) NOT NULL,
  description VARCHAR(255) NOT NULL,
  price DECIMAL(6,2) NOT NULL,
  PRIMARY KEY (id)
);

